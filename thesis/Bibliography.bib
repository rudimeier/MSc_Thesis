@ARTICLE {YGE11, 
  title = {Topologically invariant macroscopic statistics in balanced networks of conductance-based integrate-and-fire neurons}, 
  author = {Yger, Pierre and El~Boustani, Sami and Destexhe, A.  and Fr\'{e}gnac, Yves}, 

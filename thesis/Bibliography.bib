@ARTICLE {GAR12b, 
  title = {The complex Ginzburg&#8211;Landau equation: an introduction}, 
  author = {Garc�a-Morales, V.  and Krischer, K. }, 
  journal = {Contemporary Physics}, 
  year = {2012}, 
  volume = {53}, 
  number = {2}, 
  pages = {79--95}, 
  keywords = {Ginzburg}, 
  abstract = {The complex Ginzburg&#8211;Landau equation (CGLE), probably the most celebrated nonlinear equation in physics,
 
describes generically the dynamics of oscillating, spatially extended systems close to the onset of oscillations. Using
 
symmetry arguments, this article gives an easy access to this equation and an introduction into the rich spatiotemporal
 
behaviour it describes. Starting out from the familiar linear oscillator, we first show how the generic model
 
for an individual nonlinear oscillator, the so-called Stuart&#8211;Landau equation, can be derived from symmetry
 
arguments. Then, we extend our symmetry considerations to spatially extended systems, arriving at the CGLE. A
 
comparison of diffusively coupled linear and nonlinear oscillators makes apparent the source of instability in the
 
latter systems. A concise survey of the most typical patterns in 1D and 2D is given. Finally, more recent extensions of
 
the CGLE are discussed that comprise external, time-periodic forcing as well as nonlocal and global spatial coupling.}, 
} 

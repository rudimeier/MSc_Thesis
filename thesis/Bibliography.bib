  author = {Buda, Ma and Iordache, G.  and Mokkapati, S.  and Fu, Lan and Jolley, G.  and Tan, H. H.  and Jagadish, C.  and Buda, Mi}, 
  journal = {J.~Appl. Phys.}, 

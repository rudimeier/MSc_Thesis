@ARTICLE {YAN12c, 
  title = {Maximal Variability of Phase Synchrony in Cortical Networks with Neuronal Avalanches}, 
  author = {Yang, Hongdian and Shew, Woodrow L.  and Roy, Rajarshi and Plenz, Dietmar}, 

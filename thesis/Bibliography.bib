@ARTICLE {WU08, 
  title = {Observer-based synchronization in complex dynamical networks with nonsymmetric coupling}, 
  author = {Wu, Jianshe and Jiao, Licheng}, 
  journal = {Physica~A}, 
  year = {2008}, 
  volume = {386}, 
  number = {1}, 
